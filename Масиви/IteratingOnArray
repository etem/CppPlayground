#include <iostream>

int main()
{
    int myarr[5] = {1,2,3,4,5};
    
    for(int i = 0; i < 5; i++)
    {
        std::cout << myarr[i];
        std::cout << std::endl;
    }
    std::cout << "Iterating through the array and changine the numbers." << std::endl;

    for(int i = 0; i < 5; i++)
    {
        myarr[i] = 5;
    }
    for(int i = 0; i < 5; i++)
    {
        std::cout << myarr[i];
        std::cout << std::endl;
    }

}